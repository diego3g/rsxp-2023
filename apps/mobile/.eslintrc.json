{
  "extends": ["@rocketseat/eslint-config/react"],
  "rules": {
    "camelcase": "off",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": ["error"],
    "import/order": ["error", {
      "newlines-between": "always",
      "groups": [ 
          "internal", 
          "external", 
          "builtin", 
          "unknown", 
          ["parent", "sibling", "index"] 
      ],
      "pathGroups": [
        {
          "pattern": "@/**",
          "group": "unknown"
        }
      ]
    }]
  }
}
