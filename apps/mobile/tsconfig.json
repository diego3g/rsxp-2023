{
  "compilerOptions": {
    "baseUrl": "./",
    "strictNullChecks": true,
    "paths": {
      "@/*": [
        "./src/app/*",
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/assets/*": [
        "./src/assets/*"
      ],
      "@/theme/*": [
        "./src/theme/*"
      ],
      "@/app": [
        "./src/app/*"
      ],
      "@/lib": [
        "./src/lib/*"
      ],
      "@/services": [
        "./src/services/*"
      ],
      "@/utils": [
        "./src/utils/*"
      ],
    }
  },
  "extends": "expo/tsconfig.base"
}
