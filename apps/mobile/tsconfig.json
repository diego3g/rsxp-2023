{
  "compilerOptions": {
    "baseUrl": "./",
    "strictNullChecks": true,
    "paths": {
      "@/components/*": [
        "./src/components/*"
      ],
      "@/assets/*": [
        "./src/assets/*"
      ],
      "@/theme/*": [
        "./src/theme/*"
      ],
      "@/app/*": [
        "./src/app/*"
      ],
      "@/lib/*": [
        "./src/lib/*"
      ],
      "@/services/*": [
        "./src/services/*"
      ],
      "@/utils/*": [
        "./src/utils/*"
      ],
      "@/contexts/*": [
        "./src/contexts/*"
      ],
      "@/hooks/*": [
        "./src/hooks/*"
      ]
    }
  },
  "extends": "expo/tsconfig.base"
}
