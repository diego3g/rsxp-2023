{
  "compilerOptions": {
    "baseUrl": "./",
    "strictNullChecks": true,
    "paths": {
      "@/*": [
        "./src/app/*",
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/assets/*": [
        "./src/assets/*"
      ],
      "@/theme/*": [
        "./src/theme/*"
      ],
      "@/contexts/*": [
        "./src/contexts/*"
      ],
      "@/hooks/*": [
        "./src/hooks/*"
      ]
    }
  },
  "extends": "expo/tsconfig.base"
}