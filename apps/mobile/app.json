{
  "expo": {
    "name": "rsxp",
    "slug": "rsxp",
    "scheme": "myapp",
    "version": "1.0.0",
    "scheme": "rsxp",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#09090A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#09090A"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}